VK;AApBE,CAAf","sourcesContent":["<template>\n    <div class=\"columns\">\n        <div class=\"column is-2\">\n            <aside class=\"menu\">\n                <ul class=\"menu-list\">\n                    <div class=\"container\" style=\"padding: 5px\">\n                        <li>\n                            <a @click=\"pickRoom(2021)\">Room 1</a>\n                        </li>\n                        <li>\n                            <a @click=\"pickRoom(2025)\">Room 2</a>\n                        </li>\n                        <li>\n                            <a @click=\"pickRoom(2030)\">Room 3</a>\n                        </li>\n                        <li>\n                            <a @click=\"pickRoom(2050)\">Room 4</a>\n                        </li>\n                        <li>\n                            <a @click=\"pickRoom(2075)\">Room 5</a>\n                        </li>\n                    </div>\n                </ul>\n            </aside>\n        </div>\n        <div class=\"column\">\n            <div class=\"box\">\n                <div class=\"container\" v-for=\"(message, index) in messages\" :key=\"index\">\n                    <h3 class=\"subtitle\">\n                        <b>{{message.user}}</b>\n                    </h3>\n                    <div>{{message.message}}</div>\n                </div>\n                <br/>\n                <input type=\"input\" class=\"input\" v-model=\"input\" placeholder=\"Type something...\">\n                <button class=\"button is-info\" type=\"button\" :disabled=\"input == ''\" @click=\"sendMessage(this.input)\"></button>\n            </div>\n        </div>\n    </div>\n</template>\n\n<script>\nimport socket from '../../socket'\n\nexport default {\n    name: 'LibakChat',\n    created() {\n        document.addEventListener('beforeunload', this.disconnect)\n    },\n    mounted() {\n        socket.on('chat message', (msg, user) => {\n            this.messages.push({\n                user: user,\n                message: msg,\n            })\n        });\n    },\n    data() {\n        return {\n            messages: [],\n            currentUser: this.$store.state.username,\n            input: ''\n        }\n    },\n    methods: {\n        disconnect() {\n            socket.disconnect();\n        },\n        pickChat(room){\n            socket.emit('join room', room)\n        },\n        sendMessage(msg) {\n            socket.emit('message', msg, this.currentUser)\n            this.input = ''\n        }\n    }\n}\n</script>\n\n<style scoped>\n</style>"],"sourceRoot":""}]}